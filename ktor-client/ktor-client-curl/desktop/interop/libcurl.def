# curl version = 8.10.1
package = libcurl
headers = curl/curl.h
headerFilter = curl/*
staticLibraries = libcurl.a

staticLibraries.mingw = libssl.a libcrypto.a libz.a
compilerOpts.mingw = -DCURL_STATICLIB

linkerOpts.osx = -framework Security -framework SystemConfiguration

staticLibraries.linux = libssl.a libcrypto.a
linkerOpts.linux = -lz
